public interface IVariable
{ }

public interface ISetVariables<out TVariable> where TVariable : IVariable
{ }

public abstract class Constraint<TSetVariable> where TSetVariable : ISetVariables<IVariable>
{ }

public class IntVariable : IVariable
{ }

public class SetIntVariables : ISetVariables<IntVariable>
{ }

public class Constraint2 : Constraint<SetIntVariables>
{ }
